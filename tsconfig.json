{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "removeComments": true,
    "declaration": true,
    "jsx": "react"
  },
  "paths": {
    "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
  },
  "include": ["src"],
  "exclude": ["src/stories", "node_modules"],
  "files": ["src/custom.d.ts"]
}
